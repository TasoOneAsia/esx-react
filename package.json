{
  "name": "fivem-boilerplate",
  "version": "0.1.0",
  "dependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "react": "^17.0.1",
    "react-devtools": "^4.10.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:dev": "npm run format && npm run lint && webpack --config ./webpack/dev.config.js --color --progress --no-stats-cached-modules",
    "build:prod": "npm run format && npm run lint && webpack --config ./webpack/prod.config.js --color --analyze --progress --no-stats-orphan-modules",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "eslint src/**/*.ts{,x}"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^4.5.0",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "ts-loader": "^8.0.12",
    "webpack": "^5.10.3",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.2.0"
  }
}
